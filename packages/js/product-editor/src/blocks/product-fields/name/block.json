{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "woocommerce/product-name-field",
	"title": "Product name",
	"category": "widgets",
	"description": "The product name.",
	"keywords": [
		"products",
		"name",
		"title"
	],
	"textdomain": "default",
	"attributes": {
		"name": {
			"type": "string",
			"__experimentalRole": "content"
		},
		"autoFocus": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"align": false,
		"html": false,
		"multiple": true,
		"reusable": false,
		"inserter": false,
		"lock": false,
		"__experimentalToolbar": false
	},
	"editorStyle": "file:./editor.css"
}