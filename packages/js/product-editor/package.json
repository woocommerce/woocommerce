		"@wordpress/block-editor": "^9.8.0",
		"@wordpress/browserslist-config": "wp-6.0",
		"concurrently": "^7.0.0",
		"css-loader": "^3.6.0",
		"eslint": "^8.32.0",
		"jest": "^27.5.1",
		"jest-cli": "^27.5.1",
		"postcss": "^8.4.7",
		"postcss-loader": "^4.3.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-hooks^8.0.1": "link:@testing-library/react-hooks^8.0.1",
