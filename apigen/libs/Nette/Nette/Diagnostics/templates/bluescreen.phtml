			color: #C22;
		}

		#netteBluescreen pre .php-string {
			color: #080;
		}

		#netteBluescreen pre .php-int, #netteBluescreen pre .php-float, #netteBluescreen pre .php-null, #netteBluescreen pre .php-bool {
