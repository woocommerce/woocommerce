{
	"name": "woocommerce/single-product",
	"version": "1.0.0",
	"icon": "info",
	"title": "Single Product",
	"description": "Display a single product.",
	"category": "woocommerce",
	"keywords": [
		"WooCommerce"
	],
	"supports": {
		"align": [
			"wide",
			"full"
		]
	},
	"attributes": {
