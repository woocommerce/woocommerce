			"type": "boolean",
			"default": false
		},
		"productId": {
			"type": "number"
		}
	},
	"example": {
		"attributes": {
  			"isPreview": true
		}
	},
	"usesContext": [ "postId", "postType", "queryId" ],
	"textdomain": "woocommerce",
	"apiVersion": 3,
	"$schema": "https://schemas.wp.org/trunk/block.json"
}
