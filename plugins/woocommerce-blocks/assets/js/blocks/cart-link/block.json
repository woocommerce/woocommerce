{
	"name": "woocommerce/cart-link",
	"version": "1.0.0",
	"title": "Cart Link",
	"icon": "cart",
	"description": "Display a link to the cart.",
	"category": "woocommerce",
	"keywords": [ "WooCommerce" ],
	"textdomain": "woocommerce",
	"supports": {
		"html": false,
		"multiple": false,
		"typography": {
			"fontSize": true
		},
		"color": {
			"text": false,
			"link": true
		},
		"spacing": {
			"padding": true
		}
	},
	"example": {
		"attributes": {
			"isPreview": true,
			"cartIcon": "cart",
			"content": "Cart"
		}
	},
	"attributes": {
		"isPreview": {
			"type": "boolean",
			"default": false
		},
		"cartIcon": {
			"type": "string",
			"default": "cart"
		},
		"content": {
			"type": "string",
			"default": null
		}
	},
	"apiVersion": 3,
	"$schema": "https://schemas.wp.org/trunk/block.json"
}
