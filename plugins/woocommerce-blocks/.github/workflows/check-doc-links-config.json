{
	"httpHeaders": [
		{
			"urls": [
				"https://github.com/",
				"https://guides.github.com/",
				"https://help.github.com/",
				"https://docs.github.com/"
			],
			"headers": {
				"Accept-Encoding": "zstd, br, gzip, deflate"
			}
		}
	],
	"ignorePatterns": [
		{
		"pattern": "^http://localhost"
		},
		{
		"pattern": "https://www.php.net/manual/en/install.php"
		}
	],
	"retryOn429": true
}
