    "postcss-color-function": "4.0.1",
    "postcss-loader": "3.0.0",
    "prettier": "github:automattic/calypso-prettier#c56b4251",
    "prop-types": "15.7.2",
    "raw-loader": "1.0.0",
    "react-docgen": "2.21.0",
    "readline-sync": "1.4.9",
    "recast": "0.17.4",
    "replace": "1.1.0",
    "rimraf": "2.6.3",
    "rtlcss": "2.4.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "stylelint": "9.10.1",
