  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run -s install-if-no-packages && node bin/pre-commit-hook.js",
      "pre-push": "npm run -s install-if-no-packages && node bin/pre-push-hook.js"
    }
  }
}
